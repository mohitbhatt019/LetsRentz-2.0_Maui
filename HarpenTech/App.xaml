<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:HarpenTech"
              xmlns:material="http://schemas.enisn-projects.io/dotnet/maui/uraniumui/material"
             x:Class="HarpenTech.App">
    <Application.Resources>        <ResourceDictionary>            <ResourceDictionary.MergedDictionaries>                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />                <ResourceDictionary>                    <Style TargetType="Label">                        <Setter Property="FontSize" Value="12"/>                        <Setter Property="TextColor" Value="Black"/>                    </Style>                </ResourceDictionary>                <ResourceDictionary>                    <Style TargetType="Button">                        <Setter Property="TextColor" Value="#FFFFFF" />                        <Setter Property="BackgroundColor" Value="#0000FF"/>                        <Setter Property="CornerRadius" Value="10"/>                        <Setter Property="FontAutoScalingEnabled" Value="True"/>                    </Style>                </ResourceDictionary>                <ResourceDictionary>                    <Style TargetType="Entry">                        <Setter Property="TextColor" Value="#000000"/>                        <Setter Property="FontSize" Value="12"/>                        <Setter Property="FontAutoScalingEnabled" Value="True"/>                    </Style>                </ResourceDictionary>                <!--<ResourceDictionary>
                    <Style TargetType="material:AutoCompleteTextField">
                        <Setter Property="Text" Value="12" />
                    </Style>
                </ResourceDictionary>-->                            </ResourceDictionary.MergedDictionaries>        </ResourceDictionary>    </Application.Resources>
</Application>
