<?xml version="1.0" encoding="UTF-8" ?><Shell    x:Class="HarpenTech.AppShell"    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:views="clr-namespace:HarpenTech.Views"    xmlns:view="clr-namespace:HarpenTech.Views.HomePage"    xmlns:vi="clr-namespace:HarpenTech.Views.RecievePage"    xmlns:inspectcontainer="clr-namespace:HarpenTech.Views.RecievePage"     Shell.FlyoutBehavior="Flyout"      Shell.TabBarIsVisible="False" >    <Shell.FlyoutHeader>        <VerticalStackLayout>            <Image Source="rent.png"  WidthRequest="100" ></Image>            <Label Text="Lets Rentz 2.0" HorizontalTextAlignment="Center" ></Label>        </VerticalStackLayout>    </Shell.FlyoutHeader>    <FlyoutItem Title="Dashboard" FlyoutDisplayOptions="AsMultipleItems" >              <ShellContent             Title="Property"            ContentTemplate="{DataTemplate view:CMSHomePage}"           Route="HomePage"             Icon="home.png"            Shell.FlyoutBehavior="Flyout"            Shell.BackgroundColor="#0000ff"            />        <ShellContent     Title="Logout"     ContentTemplate="{DataTemplate views:LoginView}"          Route="login"     Icon="logout.png"     />    </FlyoutItem></Shell>